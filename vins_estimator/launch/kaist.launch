<launch>
    <arg name="config_path" default = "$(find feature_tracker)/../config/kaist/kaist_config.yaml" />
	  <arg name="vins_path" default = "$(find feature_tracker)/../config/../" />
    
    <node name="rvizvisualisation" pkg="rviz" type="rviz" output="log" args="-d $(find vins_estimator)/../config/kaist_rviz_config.rviz" />

    <node name="feature_tracker" pkg="feature_tracker" type="feature_tracker" output="log">
        <param name="config_file" type="string" value="$(arg config_path)" />
        <param name="vins_folder" type="string" value="$(arg vins_path)" />
    </node>

    <node name="vins_estimator" pkg="vins_estimator" type="vins_estimator" output="screen">
       <param name="config_file" type="string" value="$(arg config_path)" />
       <param name="vins_folder" type="string" value="$(arg vins_path)" />
    </node>

    <node name="pose_graph" pkg="pose_graph" type="pose_graph" output="screen">
        <param name="config_file" type="string" value="$(arg config_path)" />
        <param name="visualization_shift_x" type="int" value="0" />
        <param name="visualization_shift_y" type="int" value="0" />
        <param name="skip_cnt" type="int" value="0" />
        <param name="skip_dis" type="double" value="0" />
    </node>

    <!-- <node name="kaist_pub" pkg="vins_estimator" type="kaist_pub" output="screen" args="/home/plusai/Downloads/public_bags/KAIST/urban19/urban19-highway_data/urban19-highway /home/plusai/Downloads/public_bags/KAIST/urban19/urban19-highway_img/urban19-highway">
        <args="/home/plusai/Downloads/public_bags/KAIST/urban19/urban19-highway_data/urban19-highway /home/plusai/Downloads/public_bags/KAIST/urban19/urban19-highway_img/urban19-highway">
    </node> -->

    <!-- <node name="compress_to_raw" pkg="image_transport" type="republish" args="compressed in:=/front_left_camera/image_color raw out:=/front_left_camera/image_color"/> -->

</launch>
